@model IEnumerable<BOL.LISTING.Certification>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/Dashboard/_UserDashboard.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.ListingID)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.OwnerGuid)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IPAddress)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.GST)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ISOCertified)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CompanyPanCard)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ROCCertification)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.GomastaLicense)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AcceptTenderWork)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ListingID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.OwnerGuid)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IPAddress)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.GST)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ISOCertified)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CompanyPanCard)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ROCCertification)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.GomastaLicense)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AcceptTenderWork)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.CertificationID">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.CertificationID">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.CertificationID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
