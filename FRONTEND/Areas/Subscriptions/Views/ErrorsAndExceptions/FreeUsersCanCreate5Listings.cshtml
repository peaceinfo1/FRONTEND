@{
    ViewData["Title"] = TempData["Message"];
    Layout = "~/Views/Shared/Dashboard/_UserDashboard.cshtml";
}

<!-- Begin: Create history -->
@await Component.InvokeAsync("UserHistory", "User tried to create more than 5 listings and prohibited to do so.")
<!-- End: -->
<!-- Begin: Breadcrumb -->
@{
    ViewData["BC-1"] = "Subscriptions";
    ViewData["BC-2"] = "Listings";
    ViewData["BC-3"] = ViewData["Title"];
    ViewData["BT-Text"] = "Back To Previous Page";
    ViewData["BT-Link"] = $"{TempData["RefererUrl"]}";
    ViewData["BT-Text2"] = "Manage Listings";
    ViewData["BT-Link2"] = "/Subscriptions/Listings/Index";
}
<!-- End: -->
<!-- Page -->
<div class="page">
    <!-- Main-error-wrapper -->
    <div class="main-error-wrapper page page-h">
        <img src="~/img/Free-Users-Can-Create-5-Listings-Only.png" class="error-page" alt="error" style="width:350px;">
        <hr />
        <h2>Sorry! @TempData["Message"]</h2>
        <h6>@TempData["Description"]</h6>
        <a class="btn btn-outline-danger" href="@TempData["RefererUrl"]">Back to Previous URL</a>
    </div>
    <!-- /Main-error-wrapper -->
</div>
<!-- End Page -->